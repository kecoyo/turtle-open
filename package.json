{
  "name": "turtle-open",
  "version": "1.0.0",
  "private": true,
  "description": "egg simple project.",
  "license": "MIT",
  "author": "kecoyo",
  "scripts": {
    "ci": "npm run lint && npm run cov && npm run tsc",
    "clean": "tsc -b --clean",
    "cov": "egg-bin cov",
    "debug": "egg-bin debug",
    "dev": "egg-bin dev",
    "lint": "eslint .",
    "start": "egg-scripts start --port 7010 --title=turtle-open",
    "start-daemon": "egg-scripts start --daemon --title=turtle-open",
    "stop": "egg-scripts stop --title=turtle-open",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "tsc": "ets && tsc -p tsconfig.json"
  },
  "prettier": "@kecoyo/prettier-config",
  "dependencies": {
    "@kecoyo/egg-framework": "^1.0.2",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "@eggjs/tsconfig": "^1.3.3",
    "@kecoyo/prettier-config": "^1.0.2",
    "@types/cheerio": "^0.22.31",
    "@types/fs-extra": "^11.0.1",
    "@types/mocha": "^10.0.1",
    "cheerio": "^1.0.0-rc.12",
    "egg-bin": "^5.9.0",
    "egg-ci": "^2",
    "egg-mock": "^5.10.8",
    "eslint": "^8.45.0",
    "eslint-config-egg": "^12.2.1",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "egg": {
    "framework": "@kecoyo/egg-framework",
    "typescript": true,
    "declarations": true
  }
}
